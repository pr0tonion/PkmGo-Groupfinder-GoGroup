# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'PokemonGo Groupfinder' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PokemonGo Groupfinder
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod ‘Alamofire’
  pod 'SwiftyJSON', '~> 4.0'
  pod 'Google-Mobile-Ads-SDK'
  pod 'Cosmos', '~> 15.0'
  pod 'AZSearchView'
  target 'PokemonGo GroupfinderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PokemonGo GroupfinderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

